# Supabase
# 1) Create a project in Supabase
# 2) Project Settings > API から取得
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# REQUIRED for telemetry (server-side logging)
# IMPORTANT: This key must NEVER be exposed to the browser.
SUPABASE_SERVICE_ROLE_KEY=

# Optional: used to hash IP for uniqueness without storing raw IP
# (If empty, service role key is used as salt.)
QBU_TELEMETRY_SALT=
